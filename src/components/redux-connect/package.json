    "eslint-plugin-react": "^7.4.0",
    "immutable": "^3.8.1",
    "jest-cli": "^22.0.4",
    "prop-types": "^15.6.0",
    "raf": "^3.3.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "4.x.x",
    "react-router-config": "^1.0.0 || ^1.0.0-beta",
