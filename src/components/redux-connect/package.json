    "redux-immutable": "^4.0.0",
    "sinon": "^4.1.3"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "redux-actions": "^2.2.1",
    "redux-connect": "^7.0.0"
  },
  "jest": {
    "automock": false,
    "testEnvironment": "jsdom",
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
